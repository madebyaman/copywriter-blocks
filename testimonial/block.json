{
	"apiVersion": 2,
	"name": "create-block/testimonial",
	"version": "0.1.0",
	"title": "Testimonial",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block written with ESNext standard and JSX support â€“ build step required.",
	"attributes": {
		"credentials": {
			"type": "string",
			"source": "html",
			"selector": ".credentials"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": ".content"
		},
		"mediaID": {
			"type": "number"
		},
		"mediaURL": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"descriptionAlignment": {
			"type": "string",
			"default": "left"
		},
		"borderColor": {
			"type": "string",
			"default": "#cf2e2e"
		},
		"borderSize": {
			"type": "string"
		}
	},
	"textdomain": "copywriter-theme-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
