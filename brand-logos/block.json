{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "copywriter-blocks/brand-logos",
	"version": "0.1.0",
	"title": "Copywriter: Brand Logos",
	"category": "copywriter-blocks",
	"icon": "groups",
	"description": "Add brand logos with this block.",
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": "h2.title",
			"source": "html"
		},
		"imageOneID": {
			"type": "number"
		},
		"imageOneURL": {
			"type": "string",
			"source": "attribute",
			"attribute": "src",
			"selector": ".image-one"
		},
		"imageOneAlt": {
			"type": "string",
			"source": "attribute",
			"attribute": "alt",
			"selector": ".image-one"
		},
		"imageTwoID": {
			"type": "number"
		},
		"imageTwoURL": {
			"type": "string",
			"source": "attribute",
			"attribute": "src",
			"selector": ".image-two"
		},
		"imageTwoAlt": {
			"type": "string",
			"source": "attribute",
			"attribute": "alt",
			"selector": ".image-two"
		},
		"imageThreeID": {
			"type": "number"
		},
		"imageThreeURL": {
			"type": "string",
			"source": "attribute",
			"attribute": "src",
			"selector": ".image-three"
		},
		"imageThreeAlt": {
			"type": "string",
			"source": "attribute",
			"attribute": "alt",
			"selector": ".image-three"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#fff"
		},
		"opacity": {
			"type": "number",
			"default": 70
		}
	},
	"textdomain": "copywriter-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
